{
    "configurations": [
        {
            "type": "java",
            "name": "buytrip service configuration",
            "request": "launch",
            "mainClass": "br.com.orion.buytripconfig.BuyTripServerConfigApplication",
            "projectName": "buytrip-config",
            "vmArgs": "-noverify"
        },
        {
            "name": "bank",
            "type": "java",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "br.com.orion.bank.BankApplication",
            "projectName": "bank",
            "vmArgs": "-noverify -Dspring.profiles.active=dev"
        },
        {
            "name": "buytrip service discovery",
            "type": "java",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "br.com.orion.buytripservicediscovery.BuyTripServiceDiscoveryApplication",
            "projectName": "buytripservicediscovery",
            "vmArgs": "-noverify -Dspring.profiles.active=dev"
        },
        {
            "name": "buytrip 8180",
            "type": "java",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "br.com.orion.buytrip.BuyTripApplication",
            "projectName": "buytrip",
            "vmArgs": "-Dserver.port=8180 -noverify -Dspring.profiles.active=dev"
        },
        {
            "name": "buytrip 8181",
            "type": "java",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "br.com.orion.buytrip.BuyTripApplication",
            "projectName": "buytrip",
            "vmArgs": "-Dserver.port=8181 -noverify -Dspring.profiles.active=dev"
